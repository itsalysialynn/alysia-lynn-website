{
  "coverageDirectory": "reports",
  "coverageReporters": ["text", "cobertura"],
  "reporters": [
    "default",
    ["jest-junit", { "outputDirectory": "reports", "outputName": "jest.xml" }]
  ],
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.(t|j)sx?$": [
      "@swc/jest",
      {
        "jsc": {
          "transform": {
            "react": {
              "runtime": "automatic"
            }
          }
        }
      }
    ]
  }
}
